# API Configuration
REACT_APP_API_URL=http://localhost:3000

# Payment Gateway Keys
REACT_APP_STRIPE_PUBLIC_KEY=your_stripe_public_key
REACT_APP_STRIPE_SECRET_KEY=your_stripe_secret_key
REACT_APP_PAYPAL_CLIENT_ID=your_paypal_client_id
REACT_APP_PAYPAL_SECRET=your_paypal_secret

# AI Services
REACT_APP_DEEPSEEK_API_KEY=your_deepseek_api_key

# Blockchain Configuration
REACT_APP_WEB3_PROVIDER=your_web3_provider_url
REACT_APP_NFT_CONTRACT_ADDRESS=your_nft_contract_address

# AWS Configuration (if using AWS services)
REACT_APP_AWS_REGION=your_aws_region
REACT_APP_AWS_BUCKET=your_s3_bucket_name

# Security
REACT_APP_JWT_SECRET=your_jwt_secret
REACT_APP_ENCRYPTION_KEY=your_encryption_key

# Feature Flags
REACT_APP_ENABLE_AI_FEATURES=true
REACT_APP_ENABLE_CRYPTO_PAYMENTS=true
REACT_APP_ENABLE_FIAT_PAYMENTS=true

# Cache Configuration
REACT_APP_CACHE_DURATION=300000 # 5 minutes in milliseconds
